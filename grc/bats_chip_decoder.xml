<?xml version="1.0"?>
<block>
  <name>BATS Chip Decoder</name>
  <key>bats_chip_decoder</key>
  <category>bats</category>
  <import>import bats</import>
  <make>bats.chip_decoder($chips_per_sym)</make>

  <param>
	<name>Chips per Symbol</name>
	<key>chips_per_sym</key>
	<type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>

  <source>
    <name>out</name>
	<type>byte</type>
  </source>
</block>
